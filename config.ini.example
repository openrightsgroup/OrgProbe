[global]
# sleep between tests
interval = 1  

# Add "interface" to use a specific network interface for testing
# interface = ppp0
# interface = wlan0
# interface = eth0

[api]
# API address details
host=api.bowdlerize.co.uk
port=443
https=True
version=1.2

#Uncomment to enable amqp support
#
#[amqp]
#host=localhost
#port=5672
#ssl=False
#user=guest
#passwd=guest

# a set of probe credentials
[sampleprobe]
uuid = 01234567890123456788
secret = foobarbazquux00foobarbazquux
queue = org
#whether to run the self-test or not
selftest = True

# Add "network" to override the detected ISP (useful for mobile operators)
##network = Virgin Mobile

